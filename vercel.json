{
    "version": 2,
    "name": "bunlysia",
    "builds": [
        {
            "src": "src/index.ts",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "./src/",
            "dest": "./src/"
        },
        {
            "src": "./src/",
            "dest": "./src/"
        }
    ],
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "./src"
        }
    ]
}